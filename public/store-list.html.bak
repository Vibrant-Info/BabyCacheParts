<div ng-include="'header.html'"></div>
	<div id="content" ng-controller="storelistCtrl">
		<div id="content-header">
				<h1>Store List</h1>				
			</div>
			<div id="breadcrumb">
				<a href="#/home" title="" class="tip-bottom" data-original-title="Go to Home"><i class="fa fa-home"></i>Home</a>
				<a href="">Stors</a>
				<a href="" class="current">Store List</a>
			</div>
			<div class="row" >
				<div class="col-xs-12">
					<div class="widget-box">
						<div class="widget-title">
							<span class="icon">
								<i class="fa fa-align-justify"></i>									
							</span>
							<h5>Store List</h5>
						</div>
						<div class="widget-content nopadding " >
						<div class="row"ng-show="errmsgshow">
							<div class="col-md-12">
								<div class="alert alert-danger text-center" >Please enter any one of the fields</div>
							</div>
						</div>
							<form class="form-horizontal"  name="myForm" id="basic_validate" novalidate="novalidate">
							<div class="col-md-6">
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Address Containing</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="address" ng-model="searchst.address" >
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">City</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="city" id="city" ng-model="searchst.city">
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">State/Province</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="state" id="state" ng-model="searchst.state">
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Country</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="country" id="country" ng-model="searchst.country">
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Postal Code</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="zipcode" id="zipcode" ng-model="searchst.zipcode" ui-mask="99999">
									</div>
								</div>
								
								</div>
								
								
								<div class="col-md-6">
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Code
									</label>
									<div class="col-sm-9 col-md-9 col-lg-9 ">
										<div class="selecting-option">
										  <ui-select ng-model="searchst.code" theme="select2"  style="min-width: 100px;">
											<ui-select-match placeholder="Select Store code">{{$select.selected.code}}</ui-select-match>
											<ui-select-choices repeat="store in codevalues | propsFilter: {code: $select.search}">
											  <div ng-bind-html="store.code | highlight: $select.search"></div>
												</ui-select-choices>
											 </ui-select>
										
										  </div>
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Phone</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
									<input type="hidden" class="form-control input-sm" name="phone" id="c_phone">
										<input type="text" class="form-control input-sm " name="phone" id="phone" ui-mask="999-999-9999" ng-model="searchst.phone">
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Fax</label>
									<div class="col-sm-9 col-md-9 col-lg-9">
										<input type="text" class="form-control input-sm" name="fax" id="fax" ng-model="searchst.fax" ui-mask="999-999-9999" >
									</div>
								</div>
								<div class="form-group">
									<label class="col-sm-3 col-md-3 col-lg-3 control-label">Status	</label>
									<div class="col-sm-9 col-md-9 col-lg-9 ">
										
										<input type="radio" name="enabled"    ng-model="searchst.enabled" value=1>
									   
									   Enabled</br>
									  
										<input type="radio" name="enabled"    ng-model="searchst.enabled" value=0 >
								   
										Disabled</br>
										
										<input type="radio" name="enabled"   ng-model="searchst.enabled"  value=2>
									
										Both</br>
									</div>

									
								</div>
								
							  </div>
								<div class="form-actions col-md-12">
									<button  class="btn btn-primary" ng-click="searchValues()" >Search</button>
									<button value="Reset" class="btn btn-primary" ng-click="Reset()">Reset</button>
								</div>
							</form>
						</div>
					   
					</div>			
				</div>
				
			</div>
			<div class="row" ng-init="getValues()">
				<div class="col-lg-12">
					<div class="table-responsive">
									<table class="table table-bordered table-striped table-hover">
									<thead>
										<tr>
											
											<th>
												 Name
											</th>
											<th>
												  Code
											</th>
											<th>
												 Address
											</th>
											<th>
												 City
											</th>
											<th>
												 State\Province
											</th>
											<th>
												 Country
											</th>
											<th>
												 Postal Code
											</th>
											<th>
												 Phone
											</th>
											<th>
												 Fax
											</th>
											<th>
												 Email
											</th>
											<th>
												 Status
											</th>
											<th>
												 Options
											</th>
											
										</tr>
									</thead>
									<tbody>
										<tr ng-repeat="store in values | startFrom:(currentPage-1)*pageSize |limitTo:pageSize"  ng-show="valuesshow">
										
										<td>{{store.name}}</td>
										<td>{{store.code}}</td>
										<td>{{store.address}}</td>
										<td>{{store.city}}</td>
										<td>{{store.state}}</td>
										<td>{{store.country}}</td>
										<td>{{store.zipcode}}</td>
										<td>{{store.phone}}</td>
										<td>{{store.fax}}</td>
										<td>{{store.emailid}}</td>
										<td ng-if="store.enabled==0" class="red">Inactive </td>
										<td ng-if="store.enabled==1" class="green">Active  </i></td>
										<td ng-if="store.enabled==0"><span ng-click="checkActive(1,store.code)" class="green" ><i class="fa fa-check" data-toggle="tooltip" data-placement="bottom" title="Enable store"></i></span></td>
										<td ng-if="store.enabled==1"><span ng-click="checkActive(0,store.code)" class="red"><i class="fa fa-times"  title="Disable store"></span></td>
										</tr>
										<tr ng-show="valuesnull"><td colspan="12"> No data found</td></tr>
									</tbody>
									
									</table>
								</div>
								<uib-pagination total-items="values.length" ng-model="currentPage" items-per-page="pageSize"></uib-pagination>
				</div>
			</div>
			
	</div>
	
		
	   
		<script src="js/bootstrap.min.js"></script>
	  <!--    <script src="js/jquery.icheck.min.js"></script>  -->
		<script src="js/select2.min.js"></script>
		<script src="js/jquery.validate.js"></script>
		
		<script src="js/jquery.nicescroll.min.js"></script>
		<script src="js/babycache.js"></script>

		<script>
				$(document).ready(function(){
				
					$("select").select2();
						// $('input[type=checkbox],input[type=radio]').iCheck({
						//	checkboxClass: 'icheckbox_flat-blue',
						//	radioClass: 'iradio_flat-blue'
						//}); 
				});
		</script>
		
			</div>
<div class="row">
				<div id="footer" class="col-xs-12">
